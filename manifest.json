{
  "manifest_version": 2,
  "name": "OhTube",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["OhTube.js"]
    }
  ],
  
  "browser_action": {
    "default_icon": "OhTube.png"
  },
  
  "icons": {
    "16": "OhTube.png",
    "48": "OhTube.png",
    "128": "OhTube.png"
  }
}
